{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/transactions-lib": [
        "libs/transactions-lib/src"
      ],
      "@app/transactions-lib/*": [
        "libs/transactions-lib/src/*"
      ],
      "@app/orchestrator-lib": [
        "libs/orchestrator-lib/src"
      ],
      "@app/orchestrator-lib/*": [
        "libs/orchestrator-lib/src/*"
      ],
      "@app/blacklist-lib": [
        "libs/blacklist-lib/src"
      ],
      "@app/blacklist-lib/*": [
        "libs/blacklist-lib/src/*"
      ],
      "@app/accounts-lib": [
        "libs/accounts-lib/src"
      ],
      "@app/accounts-lib/*": [
        "libs/accounts-lib/src/*"
      ],
      "@app/rmq": [
        "libs/rmq/src"
      ],
      "@app/rmq/*": [
        "libs/rmq/src/*"
      ]
    }
  }
}